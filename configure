$as_echo "$as_me: asl_pcfiles is \"$asl_pcfiles\"" >&6;}
  fi


    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $coin_has_asl" >&5
$as_echo "$coin_has_asl" >&6; }

