: ${ac_max_here_lines=38}

# Identity of this package.
PACKAGE_NAME='Ipopt'
PACKAGE_TARNAME='ipopt'
PACKAGE_VERSION='3.2.0'
PACKAGE_STRING='Ipopt 3.2.0'
