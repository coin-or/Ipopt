_ACEOF

          cat >>confdefs.h <<\_ACEOF
#define F77_FUNC_(name,NAME) NAME ## __
_ACEOF
 ;;
  *)
          { echo "$as_me:$LINENO: WARNING: unknown Fortran name-mangling scheme" >&5
echo "$as_me: WARNING: unknown Fortran name-mangling scheme" >&2;}
          ;;
esac

